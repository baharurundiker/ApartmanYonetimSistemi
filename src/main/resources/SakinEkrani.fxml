
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.SakinEkrani" spacing="10">
    <Label fx:id="hosgeldinLabel" text="Hoşgeldiniz" />
    <Label fx:id="daireBilgisiLabel" text="Daire Bilgilerinizi buradan görebilirsiniz." />



    <TextArea fx:id="sikayetAciklamaField" promptText="Şikayet açıklamanızı buraya yazın..." />

    <TextArea fx:id="talepAciklamaField" promptText="Talep açıklamanızı buraya yazın..." />

    <Button fx:id="odemeYapButton" text="Ödeme Yap" onAction="#odemeYap" />

    <Button fx:id="sikayetOlusturButton" text="Şikayet Oluştur" onAction="#sikayetOlustur" />

    <Button fx:id="talepOlusturButton" text="Talep Oluştur" onAction="#talepOlustur" />
    <Button fx:id="duyuruListesiGorButton" text="Duyuru Gor" onAction="#duyuruGor" />
</VBox>
